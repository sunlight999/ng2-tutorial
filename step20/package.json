{
  "name": "angular-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "scripts": {
    "build:uni": "webpack --config webpack.config.uni.js",
    "serve:uni": "node src/dist/server.js",
    "start": "webpack-dev-server --inline --progress --port $PORT --host $IP",
